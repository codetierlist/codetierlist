{
    "name": "codetierlist_frontend",
    "version": "1.1.1",
    "license": "LGPL-3.0-or-later",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "lint:fix": "next lint --fix",
        "pretty": "prettier --write ."
    },
    "dependencies": {
        "@fluentui-contrib/react-data-grid-react-window": "^1.3.2",
        "@fluentui/react-components": "^9.61.6",
        "@fluentui/react-file-type-icons": "^8.13.0",
        "@fluentui/react-icons": "^2.0.297",
        "@monaco-editor/react": "^4.7.0",
        "axios": "^1.7.3",
        "is-utorid": "^1.2.2",
        "jszip": "^3.10.1",
        "magic-bytes.js": "^1.10.0",
        "monaco-editor": "^0.52.2",
        "next": "^15.3.0",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-dropzone": "^14.3.8",
        "react-grid-system": "^8.2.1",
        "react-markdown": "^10.1.0",
        "remark-gemoji": "^8.0.0",
        "remark-gfm": "^4.0.1",
        "use-local-storage": "^3.0.0"
    },
    "devDependencies": {
        "@types/node": "^22.14.1",
        "@types/react": "^18.3.2",
        "@types/react-dom": "^18.3.0",
        "@typescript-eslint/eslint-plugin": "^7.7.0",
        "@typescript-eslint/parser": "^7.18.0",
        "codetierlist-types": "../types",
        "eslint": "^9.26.0",
        "eslint-config-next": "^14.2.3",
        "prettier": "^3.5.3",
        "typescript": "^5.8.3"
    },
    "prettier": {
        "arrowParens": "always",
        "endOfLine": "auto",
        "printWidth": 90,
        "singleQuote": true,
        "tabWidth": 4,
        "trailingComma": "es5"
    },
    "eslintConfig": {
        "extends": [
            "eslint:recommended",
            "plugin:@typescript-eslint/recommended",
            "next/core-web-vitals",
            "plugin:react-hooks/recommended",
            "plugin:react/recommended",
            "plugin:react/jsx-runtime"
        ],
        "parser": "@typescript-eslint/parser",
        "plugins": [
            "@typescript-eslint"
        ],
        "rules": {
            "@typescript-eslint/no-unused-vars": [
                "error",
                {
                    "argsIgnorePattern": "^_",
                    "varsIgnorePattern": "^_",
                    "caughtErrorsIgnorePattern": "^_"
                }
            ],
            "no-console": [
                "warn"
            ],
            "no-shadow": "off",
            "@typescript-eslint/no-shadow": [
                "error"
            ],
            "no-use-before-define": "off",
            "@typescript-eslint/no-use-before-define": [
                "error"
            ],
            "react/jsx-filename-extension": [
                "warn",
                {
                    "extensions": [
                        ".tsx"
                    ]
                }
            ]
        },
        "env": {
            "browser": true
        },
        "settings": {
            "react": {
                "pragma": "React",
                "version": "detect"
            }
        }
    }
}
